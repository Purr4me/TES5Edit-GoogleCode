\par \page\pard\plain \s2\rtlch\afs20\lang255\ltrch\dbch\afs20\langfe255\loch\fs20{\super #{\*\footnote #\pard\plain \s47\li283\ri0\fi-283\rtlch\ltrch\dbch\loch\fs20 {\ltrch\loch\f1  basic_howto}}}
